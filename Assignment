   ],
   "source": [
    "def binary(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        binary(n//2)\n",
    "    print(n % 2,end = '')\n",
    "\n",
    "Nbr = int(input(\"Enter your Number: \"))\n",
    "binary(Nbr)\n"
   ]
