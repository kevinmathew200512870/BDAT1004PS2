   ],
   "source": [
    "\n",
    "def fileLength(fileName):\n",
    "    try:                                           \n",
    "        infile=open(fileName)\n",
    "        data=infile.read()\n",
    "        ln=len(data)\n",
    "        infile.close()\n",
    "        print(ln)\n",
    "    except IOError:                                  \n",
    "        print('File ' + fileName + ' not found.')     \n",
    "                                                   \n",
    "        \n",
    "fileLength('Filelength.txt')  \n",
    "fileLength('filelent.txt')     \n"
   ]
